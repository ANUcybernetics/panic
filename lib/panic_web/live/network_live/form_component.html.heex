<div>
  <.form
    let={f}
    for={@changeset}
    id="starting-input"
    phx-target={@myself}
    phx-change="validate"
    phx-submit="create_run"
  >

    <.form_field type="text_input" form={f} field={:input} />
    <.button class="mb-8" type="submit" phx_disable_with="Saving..." label="Save" />
  </.form>
</div>
